{
    if ( m_pHEAD == 0 ) {
   	    m_pHEAD = m_pTAIL = NodeToAdd;
        NodeToAdd->m_pNEXT = 0;
        NodeToAdd->m_pPREV = 0;
    } else {
        NodeToAdd->m_pNEXT = 0;
		NodeToAdd->m_pPREV = m_pTAIL;
        m_pTAIL->m_pNEXT = NodeToAdd;

        m_pTAIL = NodeToAdd;
    }

    iNodeCount ++;
