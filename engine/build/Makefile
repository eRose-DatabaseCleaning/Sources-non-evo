	rm -rf ./sample/znviewer/Release
	rm -rf ./sample/znviewer/makeDLL_debug
	rm -rf ./sample/znviewer/makeLIB_release
	rm -rf ./sample/znviewer/static_lib_multi_debug
	rm -rf ./sample/znviewer/static_lib_multi_release
	rm -rf ./sample/mesh_converter/Debug
	rm -rf ./sample/mesh_converter/Release
	rm -rf ./sample/mesh_info/Debug
	rm -rf ./sample/mesh_info/Release
	rm -rf ./sample/mfc_sample/Debug
	rm -rf ./sample/mfc_sample/Release
